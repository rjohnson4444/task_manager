<div class="well">
  <h1 class='text-center'>All Tasks</h1>
  <table class='table table-condensed'>
    <tr>
      <th><h1>#</h1></th>
      <th><h1>Tasks</h1></th>
    </tr>
    <% @tasks.each do |task| %>
    <tr>
      <td><%= task.id %></td>
      <td><a href="/tasks/<%= task.id %>"><%= task.title %></a></td>
    </tr>
    <% end %>
  </table>
</div>
